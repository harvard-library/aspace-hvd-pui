<section  id="header">
  <div class="container logo-container">
    <div class="row" id="logo">
      <div class="col-xl-9 col-lg-9 col-md-8 col-sm-8 hollis-row">
        <div class="hollis-link">
          <% unless request.original_fullpath == '/' %>
            <a title="<%=t('brand.title_link_text') %>" href="<%= AppConfig[:public_proxy_url] %>"><% end %>
              <span class="header-title"><%= t('brand.title') + ' ' %></span>
              <span class="title-subtext"><%= t('brand.title_subtext')%></span>
              <% unless request.original_fullpath == '/' %>
            </a>
          <% end %>
        </div>
      </div>
    <div class="logo-row" id="logo">
      <div class="col-xl-3 col-lg-3 col-md-4 col-sm-4 harvard-logo"><a href="<%= t('brand.logo_link') %>"
         title="<%= t('brand.logo_link_title') %>"><img alt="<%= t('brand.logo_alt') %>" src="/assets/hl_logo.svg"/></a>
      </div>
    </div>
  </div>
  <div class="row" id="pui_brand">
    <div class="nav-row"><%= render partial: 'shared/head_nav' %></div>
  </div>
</section>
